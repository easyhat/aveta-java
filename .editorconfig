# Top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
trim_trailing_whitespace = true

# Java files
[*.java]
indent_style = space
indent_size = 2
continuation_indent_size = 8
max_line_length = 120


# Markdown files
[*.md]
trim_trailing_whitespace = false

